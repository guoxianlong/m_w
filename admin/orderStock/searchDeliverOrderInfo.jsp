<%@ page language="java" pageEncoding="UTF-8"%>
<%@ taglib prefix="mmb" uri="http://www.maimaibao.com/core" %>
<!DOCTYPE html>
<html>
<head>
<title>配送信息查询</title>
<jsp:include page="../rec/inc/easyui.jsp"></jsp:include>
<script type="text/javascript">
function searchFun() {
	var packageCode = $("#packageCode").val();
	var orderCode = $("#orderCode").val();
	if ($.trim(packageCode) == "" && $.trim(orderCode) == "") {
		$.messager.show({
			msg : "请填写包裹单号或订单号",
			title : '提示'
		});
		return false;
	}
	$.ajax ({
		url:"${pageContext.request.contextPath}/OrderStockController/getDeliverOrderInfo.mmx",
		data:"packageCode=" + packageCode + "&orderCode=" + orderCode,
		dataType: "json", //返回的数据的形式                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
        success: function(data) { 
        	if (data['tip']) {
        		$.messager.show({
        			msg : data['tip'],
        			title : '提示'
        		});
        		$("#deliverInfo").html("");
        	} else {
        		var returnInfo = '';
        		returnInfo +="<fieldSet>";
    			returnInfo += "&nbsp;&nbsp;";
     			returnInfo += data['rows'][0];
    			returnInfo += "<br/>";
    			returnInfo += "<hr/>";
        		for (var i = 1; i < data['total']; i ++) {
        			returnInfo += "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;";
         			returnInfo += data['rows'][i];
        			returnInfo += "<br/>";
        			returnInfo += "<hr/>";
        		}
        		returnInfo +="</fieldSet>"
	        	$("#deliverInfo").html(returnInfo);
        	}
        }
	});
}

</script>
</head>
<body>
	<fieldset>
		<legend>筛选</legend>
		<table class="tableForm">
			<tr>
				<th>包裹单号：</th>
				<td>
					<input name='packageCode' id='packageCode' style="width:152px"/>
				</td>
				<th>订单编号：</th>
				<td>
					<input name='orderCode' id='orderCode' style="width:152px"/>
				</td>
				<td>
					<mmb:permit value="2162">
					<a href="javascript:void(0);" class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true" onclick="searchFun();">查询</a>
					</mmb:permit>
				</td>
			</tr>
		</table>
	</fieldset>
	<br/>
	<br/>
	<div id='deliverInfo'></div>
</body>
</html>